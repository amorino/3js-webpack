{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "TweenMax": true,
    "TweenLite": true,
    "TimelineMax": true,
    "Sine": true,
    "Power1": true,
    "Power2": true,
    "Power3": true,
    "Power4": true,
    "Back": true,
    "Elastic": true,
    "Linear": true,
    "THREE": true
  },
  "rules": {
    "no-unused-vars": 1,
    "no-console": 0,
    "max-len": 0,
    "semi": ["error", "never"],
    "arrow-body-style": 0,
    "indent": [1, 2, { "SwitchCase": 1 }],
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-named-default": 0,
    "no-tabs": 0,
    "no-return-assign": 0,
    "no-unused-expressions": 0,
    "comma-dangle": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack/webpack.config.test.babel.js"
      }
    }
  }
}
