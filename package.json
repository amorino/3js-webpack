{
  "name": "3js-webpack",
  "version": "1.0.0",
  "description": "A boilerplate for three.js using Webpack",
  "main": "src/main.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && npm run build:webpack && open .",
    "build:webpack": "NODE_ENV=production webpack --config webpack/webpack.config.production.babel.js --progress --colors -p",
    "dev": "NODE_ENV=development node -r babel-register webpack/server.js",
    "start": "npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amorino/3js-webpack.git"
  },
  "keywords": [
    "three.js",
    "webpack",
    "stylus"
  ],
  "author": "Erick Baz√°n",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amorino/3js-webpack/issues"
  },
  "homepage": "https://github.com/amorino/3js-webpack#readme",
  "dependencies": {
